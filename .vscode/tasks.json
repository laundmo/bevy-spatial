{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run aabb_visualize",
      "detail": "Runs the aabb_visualize example",
      "type": "shell",
      "command": "cargo run --features debug --example aabb_visualize",
      "options": {
        "env": {
          "RUST_LOG": "warn,{{crate_name}}=trace"
        }
      },
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared",
        "showReuseMessage": true,
        "clear": true,
        "focus": true
      }
    },
